{
  "manifest": {
    "prefix": "web.manifest",
    "body": [
      "{",
      "\"background_color\": \"#fff\",",
      "\"display\": \"standalone\",",
      "\"orientation\":\"portrait\",",
      "\"theme_color\": \"#fff\",",
      "\"short_name\": \"App Name\",",
      "\"name\": \"App Name\",",
      "\"description\": \"About your App\",",
      "\"lang\": \"en-US\",",
      "\"icons\": [",
      "  {",
      "   \"src\": \"images/icons/icon-192x192.png\",",
      "   \"sizes\": \"192x192\",",
      "   \"type\": \"image/png\"",
      "  },",
      "  {",
      "   \"src\": \"images/icons/icon-512x512.png\",",
      "   \"sizes\": \"512x512\",",
      "   \"type\": \"image/png\"",
      "  }",
      "],",
      "\"screenshots\": [",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-1.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  },",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-2.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  },",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-3.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  }",
      "],",
      "\"shortcuts\": [",
      "  {",
      "    \"name\": \"Shortcut Name\",",
      "    \"short_name\": \"Shortcut Name\",",
      "    \"description\": \"Shortcut Description\",",
      "    \"url\": \"https://example.com/\",",
      "\"icons\": [",
      "  {",
      "   \"src\": \"images/icons/icon-192x192.png\",",
      "   \"sizes\": \"192x192\"",
      "  },",
      "  {",
      "   \"src\": \"images/icons/icon-512x512.png\",",
      "   \"sizes\": \"512x512\"",
      "  }",
      "]",
      "  }],",
      "\"start_url\": \"index.html\",",
      "\"scope\": \"./\"",
      "}"
    ],
    "description": "Create Web Manifest"
  },

  "Native App Install Banner": {
    "prefix": "native.install",
    "body": [
      "$1",
      " \"prefer_related_applications\": true,",
      " \"related_applications\": [",
      "   {",
      "     \"platform\": \"play\",",
      "     \"id\": \"your_native_app_id\"",
      "    }",
      " ]",
      " $1"
    ],
    "description": "Native App Install Banner"
  },

  "Web Share Target API Basic": {
    "prefix": "web.share.basic",
    "body": [
      "$1",
      " \"share_target\": {",
      "   \"action\": \"/share-target/\",",
      "   \"method\": \"GET\",",
      "   \"params\": [",
      "     {",
      "       \"title\": \"title\",",
      "       \"text\": \"text\",",
      "       \"url\": \"url\"",
      "     }",
      "   ]",
      " },",
      "$1"
    ],
    "description": "Web Share Target API"
  },
  "Web Share Target API APP": {
    "prefix": "web.share.app",
    "body": [
      "$1",
      " \"share_target\": {",
      "   \"action\": \"/bookmark/\",",
      "   \"method\": \"POST\",",
      "   \"enctype\": \"multipart/form-data\",",
      "   \"params\": [",
      "     {",
      "       \"url\": \"url\"",
      "     },",
      "   ]",
      " },",
      "$1"
    ],
    "description": "Web Share Target API"
  },

  "Web Share Target API Files": {
    "prefix": "web.share.files",
    "body": [
      "$1",
      " \"share_target\": {",
      "   \"action\": \"/cgi-bin/aggregate/\",",
      "   \"method\": \"POST\",",
      "   \"enctype\": \"multipart/form-data\",",
      "   \"params\": {",
      "       \"title\": \"name\",",
      "       \"text\": \"description\",",
      "       \"url\": \"url\"",
      "     },",
      "   \"files\": [",
      "     {",
      "       \"name\": \"file\",",
      "       \"type\": \"image/png\"",
      "     }",
      "   ]",
      " },",
      "$1"
    ],
    "description": "Web Share Target API"
  },

  "Add PWA Screenshots": {
    "prefix": "pwa.screenshots",
    "body": [
      "$1",
      "\"screenshots\": [",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-1.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  },",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-2.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  },",
      "  {",
      "    \"src\": \"images/screenshots/screenshot-3.png\",",
      "    \"sizes\": \"320x640\",",
      "    \"type\": \"image/png\"",
      "  }",
      "]"
    ],
    "description": "Add PWA Screenshots"
  }
}
